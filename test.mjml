        .header-img-text div{ font-size: 20px !important; line-height: 25px !important; height: 100% !important; text-align: center !important; }
        .content-title  div{ font-size: 20px !important; line-height: 25px !important;  }
        .content-title > td {padding-left: 20px !important; padding-right: 20px !important; }
        .content-text > td {padding-left: 20px !important; padding-right: 20px !important; padding-bottom: 5px !important;}
        .content-text.content-text-info > td { padding-bottom: 0 !important;}
        .content-text div { font-size: 14px !important; line-height: 19px !important; }
        .content-wrap > table > tbody > tr > td { padding-bottom: 20px !important; padding-top: 30px !important; }

        .divider > table > tbody > tr > td{ padding-left: 0 !important; padding-right: 0 !important; }
        .footer-content > table > tbody > tr > td{ padding-left: 20px !important; padding-right: 20px !important; padding-top: 30px !important; }
        .footer-law > table > tbody > tr > td{ padding-left: 0 !important; padding-right: 0 !important; }
        .footer-socials > table > tbody > tr > td  { padding-left: 15% !important; padding-right: 15% !important; }
        .logo-wrap > table > tbody > tr > td{ padding-left: 20px !important; padding-right: 20px !important; }
        .img-mobile > td  > table > tbody > tr > td { width:100% !important;}
        .img-mobile img { height:auto !important; }
      }
    </mj-style>
